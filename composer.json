{
    "name": "ll-kuma-ll/laravel-form-view",
    "description": "The Form View for Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LLkumaLL\\FormView\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LLkumaLL\\FormView\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "illuminate/view": "^6.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LLkumaLL\\FormView\\FormViewServiceProvider"
            ]
        }
    }
}
